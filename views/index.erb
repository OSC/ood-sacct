<h1><%= title %></h1>

Output generated: <%= Time.now %>

<a type="button" class="btn btn-sm btn-default" onClick="window.location.reload()">&#x21bb; Refresh</a>

<pre>
$ <%= @command %>
</pre>

<table class="table table-bordered">
  <tr>
    <th>JOBID</th>
    <th>JOBNAME</th>
    <th>PARTITION</th>
    <th>ACCOUNT</th>
    <th>CPU</th>
    <th>STATE</th>
    <th>EXIT CODE</th>
  </tr>
  <% @processes.each do |app| %>
  <tr>
    <td><a href="<%= url("show/") %><%= app.jobid %>"><%= app.jobid %></a></td>
    <td><%= app.jobname %></td>
    <td><%= app.partition %></td>
    <td><%= app.account %></td>
    <td><%= app.cpus %></td>
    <td><%= app.state %></td>
    <td><%= app.exit_code %></td>
  </tr>
  <% end %>

</table>
